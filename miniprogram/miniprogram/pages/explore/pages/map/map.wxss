/* pages/explore/pages/map/map.wxss */
.map {
  height: 100vh;
  width: 100%;
}

.rp-search__pickers {
  position: absolute;
  bottom: 0;
  width: calc(100% - 6px);
  margin: 0 4px;
}

.rp-search__picker {
  display: inline-block;
  width: 49%;
  margin: 1px;
  border-radius: 8px;
  overflow: hidden;
}
