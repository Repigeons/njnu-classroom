/* pages/empty/empty.wxss */

.rp-title {
  font-weight: 700;
}

.rp-dialog {
  visibility: hidden;
  opacity: 0;
  transition: opacity .3s;
}

.rp-dialog_show {
  visibility: visible;
  opacity:1;
}

.rp-dialog__bd {
  text-align: left;
  margin: 5px 0 15px 0;
  padding: 0 20px 0 24px;
}

.rp-layer {
  position:absolute;
  right:16px;
  opacity: 0.8;
}

.rp-cells {
  position: relative;
  background-color: #fff;
  margin: 8px 8px 8px 8px;
  border-radius: 16px;
}
